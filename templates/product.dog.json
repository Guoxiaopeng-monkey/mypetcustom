                "open_by_default": false,
                "icon": "none",
                "width": 20
              },
              "blocks": {
                "text_fkBFWX": {
                  "type": "text",
                  "name": "t:names.text",
                  "settings": {
                    "text": "<p>Each piece comes in a beautifully designed gift box, ready to be gifted directly. We also offer an optional <em>custom message card service</em> to make your present even more heartfelt.</p>",
                    "width": "100%",
                    "max_width": "none",
                    "alignment": "left",
                    "type_preset": "rte",
                    "font": "var(--font-body--family)",
