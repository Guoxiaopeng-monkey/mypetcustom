              "name": "t:names.accordion_dash_row",
              "settings": {
                "heading": "4. Can I see a preview during the process?",
                "open_by_default": false,
                "icon": "none",
                "width": 20
              },
              "blocks": {
                "text_39rebE": {
                  "type": "text",
                  "name": "t:names.text",
                  "settings": {
                    "text": "<p>Absolutely! We’ll send you a preview photo before the final completion. You can request <strong>up to two rounds of revisions</strong> to ensure the artwork perfectly captures your pet’s look and personality.</p>",
                    "width": "100%",
                    "max_width": "none",
                    "alignment": "left",
                    "type_preset": "rte",
                    "font": "var(--font-body--family)",
                    "font_size": "1rem",
