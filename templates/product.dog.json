            },
            "accordion_row_bLpQXN": {
              "type": "_accordion-row",
              "name": "t:names.accordion_row",
              "settings": {
                "heading": "10. How will I know when my order has shipped?",
                "open_by_default": false,
                "icon": "none",
                "width": 20
              },
              "blocks": {
                "text_qf4Mhq": {
                  "type": "text",
                  "settings": {
                    "text": "<p>Once your order is shipped, you’ll receive a tracking number via email. Standard delivery usually takes <strong>5–10 days</strong>. If you’d like to upgrade to express shipping, please contact our team after placing your order (additional fees may apply).</p>",
                    "width": "100%",
                    "max_width": "normal",
                    "alignment": "left",
