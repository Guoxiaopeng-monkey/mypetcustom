              ]
            },
            "accordion_row_gHNB3V": {
              "type": "_accordion-row",
              "name": "t:names.accordion_dash_row",
              "settings": {
                "heading": "7. How should I care for my felted artwork?",
                "open_by_default": false,
                "icon": "none",
                "width": 20
              },
              "blocks": {
                "text_Eg43iP": {
                  "type": "text",
                  "name": "t:names.text",
                  "settings": {
                    "text": "<p>Keep your felted pet in a dry, shaded place away from moisture and direct sunlight. To remove dust, gently clean it with a soft brush. Please avoid washing or pressing the artwork to maintain its original shape.</p>",
                    "width": "100%",
                    "max_width": "none",
                    "alignment": "left",
                    "type_preset": "rte",
                    "font": "var(--font-body--family)",
                    "font_size": "1rem",
                    "line_height": "normal",
                    "letter_spacing": "normal",
                    "case": "none",
