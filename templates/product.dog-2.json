      "blocks": {
        "text_7XqzUa": {
          "type": "text",
          "name": "t:names.heading",
          "settings": {
            "text": "<h3>Frequently asked questions</h3>",
            "width": "fit-content",
            "max_width": "narrow",
            "alignment": "left",
            "is_default_setting": true,
            "title_size_pc": "48",
            "title_size_mb": "32",
            "type_preset": "rte",
            "font": "var(--font-body--family)",
            "font_size": "1rem",
            "line_height": "normal",
            "letter_spacing": "normal",
