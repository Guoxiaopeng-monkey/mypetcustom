    "section_xikd3w": {
      "type": "section",
      "blocks": {
        "text_7XqzUa": {
          "type": "text",
          "name": "t:names.heading",
          "settings": {
            "text": "<h3>Frequently asked questions</h3>",
            "width": "fit-content",
            "max_width": "narrow",
            "alignment": "left",
            "type_preset": "rte",
            "font": "var(--font-body--family)",
            "font_size": "1rem",
            "line_height": "normal",
            "letter_spacing": "normal",
